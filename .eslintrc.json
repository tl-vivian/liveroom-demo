{
  "extends": ["eslint:recommended", "prettier", "prettier/react","plugin:react/recommended"],
  "plugins": [],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
